<h4>商品一覧</h4>
<%= link_to "新規登録", new_admin_item_path %>

<table>
<thead>
<tr>
<th>商品ID</th>
<th>商品名</th>
<th>税抜価格</th>
<th>ジャンル</th>
<th>ステータス</th>
</tr>
</thead>


<tbody>
<% @item.each do |item| %>
<tr>
 <th><%= item.id %></th>

 <th><%= link_to item.name, admin_item_path(item.id)  %></th>

 <th><%= item.price %></th>
</tr>
<% end %>
<%#= paginate admin_items %>
</tbody>

</table>
