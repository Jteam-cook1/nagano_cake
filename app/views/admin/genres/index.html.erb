<h3>ジャンル一覧・追加</h3>

 <%= form_with model: @genre,url: admin_genres_path do |f| %>
  <h4>ジャンル名</h4>
  <%= f.text_field :name %>
  <%= f.submit '新規登録' %>
  <% end %>
<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
    </tr>
   <p>ジャンル</p>
  </thead>
  <tbody>
   <% @genres.each do |genre| %>
    <tr>
    <td>  
      <%= genre.name %>
    <%= link_to "編集", edit_admin_genre_path(genre.id) %>
    </td>
    </tr>
   <% end %>
  </tbody>
</table>
  
